.inversion-row
  position: relative
  background: rgba(243, 243, 243, .6)
  .accordion
    position: relative
    z-index: 10
  .inversion-row-content
    background: white
    padding: 37px
    position: relative
    z-index: 10001
    .inversion-inner-left
      margin-right: 5%
      width: 24%
      .chico-inversion
        margin: 0 !important
    .inversion-inner-left
    .inversion-inner-right
      display: inline-block
      vertical-align: top
    .inversion-inner-right
      width: 70%
    .inversion-inner-total
      width: 100%
      .avatar-logo
        margin: 0 0 16px 0

    .titulo
      left: 0
      position: absolute
      top: -156px
  .member.inversion
    border: 1px solid #979797
    cursor: pointer
    display: inline-block
    height: 370px
    margin-right: .5%
    padding: 27px 17px
    transform-origin: center
    transition: .25s
    vertical-align: top
    width: 19%
    &:hth-child(last)
      margin-right: 0
    &:hover
      background: $verde
      //transform: scale(1.01)
      .avatar-logo
        background: white
        border: 0
        color: $verde
      .inversion-row-content
        .avatar-logo
          background: transparent
          background: white
          border: 1px solid #979797
          color: $azul
        .name
          color: $gris
          h3
            color: $gris
            span
              color: $gris
            .bisel
              background: $gris
      .name
        color: white
        h3
          color: white
          span
            color: white
          .bisel
            background: white
      .sub-name
        p
          color: white
    &.active
      .slots
        pointer-events: auto
        opacity: 1
        visibility: visible
        z-index: 1
        .back-layer
          z-index: 101
    .avatar
      display: block
      margin: 0 auto
      height: 115px
      width: 115px
    .name
      font-size: 18px
      margin: 21px 0 0 0
      text-align: left
    .slots
      left: 0
      pointer-events: none
      position: absolute
      opacity: 0
      top: 0
      visibility: hidden
      width: 100%
      z-index: -1
    .sub-name
      p
        font-size: 13px
        font-weight: 100
  .table-inversion
    border: 0
    border-bottom: 1px solid rgba(78,93,106, .5)
    padding: 0
    table
      margin-bottom: 10px
    th
      padding: 5px !important
    thead
