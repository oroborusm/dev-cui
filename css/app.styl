@import 'normalize.css'

$azul = #003473
$gris = #4E5D6A
$gris2 = #1F2C36
$verde = #43A72D

*
  box-sizing: border-box
  -webkit-font-smoothing: antialiased
body
html
  height: 100%
  font-family: 'Montserrat', sans-serif
  font-size: 16px
header
  //background: pink
  height: 100%
  //min-height: 620px
  width: 100%
.aa
  background: rgba(243, 243, 243, .6)
  background-image: url('../img/aa.jpg')
  background-position: center
  background-size: cover
  padding: 0 !important
  position: relative
  //z-index: 1002
  .aa-white
    background: rgba(243, 243, 243, .88)
    padding-bottom: 90px
    padding-top: 40px
  .a-inner-left
    width: 39%
  .a-inner-left
  .a-inner-rigth
    display: inline-block
    vertical-align: top
    p
      margin-bottom: 25px
  .a-inner-rigth
    width: 60%
  .aa-title
    color: $azul
    font-size: 36px
    margin-bottom: 45px
    span
      color: $verde
      display: block
      font-size: 34px
      font-weight: 100

.absolute
  position: absolute

.ac-custom label
  display: inline-block
  position: relative
  font-size: 18px
  font-weight: 100
  padding: 0 0 0 60px
  vertical-align: top
  color: #003473
  cursor: pointer
  transition: color 0.3s

.ac-custom input[type="checkbox"]
.ac-custom input[type="radio"]
.ac-custom label::before
  width: 50px
  height: 50px
  top: 50%
  left: 10px
  margin-top: -25px
  position: absolute
  cursor: pointer

.ac-custom input[type="checkbox"]
.ac-custom input[type="radio"]
  opacity: 0
  display: inline-block
  vertical-align: middle
  z-index: 100

.ac-custom label::before
  background: $verde
  //border: 4px solid $verde
  border-radius: 50%
  content: ''
  height: 20px
  margin-top: -10px
  position: absolute
  top: 50%
  transition: opacity 0.3s
  width: 20px
.ac-custom svg path
  fill: transparent
  stroke: $verde
  stroke-dashoffset: 0
  stroke-linecap: round
  stroke-linejoin: round
  stroke-width: 5px
.arrows
  bottom: 130px
  position: absolute
  text-align: right
  width: 100%
  p
    color: white
    cursor: pointer
    display: inline-block
    font-weight: 800
    text-align: center
    text-shadow: 0 0 16px rgba(0,0,0, .8)
    width: 50px
.back-layer
  background: rgba(black, .6)
  bottom: 0
  left: 0
  position: fixed
  right: 0
  top: 0
.chico
  .titulo-gral
    font-size: 1.1em
    .bisel
      margin-top: 13px
      width: 72px
    .titulo-bold
      font-size: 22px
.chico-inversion
  .titulo-gral
    font-size: 18px
    .bisel
      margin-top: 13px
      width: 72px
    .titulo-bold
      font-size: 18px
.asesoria
  //background: #F3F3F3
  background: rgba(243, 243, 243, 0.7)
  display: block
  margin-top: 40px
  opacity: 1
  padding: 20px
  position: relative
  transition: .35s
  visibility: visible
  &.fixed
    //left: 0
    margin-right: -16px
    margin-top: 0
    position: fixed
    top: 120px
    z-index: 1001
  &.opacidad
    opacity: 0
    visibility: hidden
  h2
    color: $azul
    font-size: 18px
    text-align: center
    text-transform: uppercase
  label
    color: $azul
    display: block
    font-size: 14px
    margin-top: 15px
    text-align: left
  input
    width: 100%
  [type="submit"]
    background: $azul
    border: 0
    color: white
    display: block
    font-family: 'Montserrat'
    font-size: 14px
    font-weight: 700
    margin: 36px auto 0
    padding: 18px
    text-transform: uppercase
    transition: .2s
    transform-origin: center
    width: 217px
