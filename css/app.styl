            vertical-align: top
          .pregunta-inner-right
            border-top: 2px solid #f3f3f3
            width: 70%
            .button-next
              background-color: $verde !important
              border: 0 !important
              color: white !important
              float: right
              font-size: 14px
              margin-top: 16px
              padding: 16px 24px
              transition: .35s
              &[disabled]
                background: rgba($verde, .4) !important
            .check-line
              border-bottom: 2px solid #f3f3f3
              list-style: none
              //min-height: 60px
              margin: 0
              padding: 0
              position: relative
              p
                font-size: 14px
                //height: 60px
                line-height: 30px
                margin: 0
                padding: 10px 0
                &:hover
                  background: #f3f3f3
                input
                  background: $verde
                  display: inline-block;
                  height: 30px
                  //left: 10px
                  margin-top: -15px
                  opacity: 0;
                  position: absolute
                  vertical-align: middle;
                  width: 30px
                  -webkit-appearance: none;
                  z-index: 100;
                label
                  //line-height: 60px
                svg
                  height: 40px
